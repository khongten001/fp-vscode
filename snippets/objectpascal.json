{
    "begin-end": {
        "prefix": "begin-end",
        "body": [
            "begin",
            "  $1",
            "end;"
        ],
        "description": "snippets"
    },
    "if-then-begin-end": {
        "prefix": "if-then-begin-end",
        "body": [
            "if $1 then",
            "begin",
            "  $2",
            "end;"
        ],
        "description": "snippets"
    },
    "if-then-begin-end-else-begin-end": {
        "prefix": "if-then-begin-end-else-begin-end",
        "body": [
            "if $1 then",
            "begin",
            "  $2",
            "end",
            "else",
            "begin",
            "  $3",
            "end;"
        ],
        "description": "snippets"
    },
    "with-do-begin-end": {
        "prefix": "with-do-begin-end",
        "body": [
            "with $1 do",
            "begin",
            "  $2",
            "end;"
        ],
        "description": "snippets"
    },
    "case-end": {
        "prefix": "case-end",
        "body": [
            "case $1 of",
            "  $2",
            "end;"
        ],
        "description": "snippets"
    },
    "for-to-do-begin-end": {
        "prefix": "for-to-do-begin-end",
        "body": [
            "for $1 := $2 to $3 do",
            "begin",
            "  $4",
            "end;"
        ],
        "description": "snippets"
    },
    "for-in-do-begin-end": {
        "prefix": "for-in-do-begin-end",
        "body": [
            "for $1 in $2 do",
            "begin",
            "  $3",
            "end;"
        ],
        "description": "snippets"
    },
    "while-do-begin-end": {
        "prefix": "while-do-begin-end",
        "body": [
            "while $1 do",
            "begin",
            "  $2",
            "end;"
        ],
        "description": "snippets"
    },
    "repeat-until": {
        "prefix": "repeat-until",
        "body": [
            "repeat",
            "  $1",
            "until"
        ],
        "description": "snippets"
    },
    "try-finally-end": {
        "prefix": "try-finally-end",
        "body": [
            "try",
            "  $1",
            "finally",
            "  $2",
            "end;"
        ],
        "description": "snippets"
    },
    "try-except-end": {
        "prefix": "try-except-end",
        "body": [
            "try",
            "  $1",
            "except", 
            "  $2",
            "end;"
        ],
        "description": "snippets"
    },
    "iinterface-end": {
        "prefix": "iinterface-end",
        "body": [
            "I${1:Name} = interface$2['{${UUID}}']", 
            "end;"
        ],
        "description": "snippets"
    },
    "tclass-private-public-end": {
        "prefix": "tclass-private-public-end",
        "body": [
            "T${1:Name} = class",
            "private", 
            "public",
            "  constructor Create;",
            "  destructor Destroy; override; $2",
            "end;"
        ],
        "description": "snippets"
    }, 
    "constructor": {
        "prefix": "constructor",
        "body": [
            "constructor Create(${1:Params}); ${2:override};"
        ],
        "description": "snippets"
    },
    "destructor": {
        "prefix": "destructor",
        "body": [
            "destructor Destroy; override;$0"
        ],
        "description": "snippets"
    },
    "function": {
        "prefix": "function",
        "body": [
            "function ${1:Name}(${2:Params}):${3:string};"
        ],
        "description": "snippets"
    },
    "procedure": {
        "prefix": "procedure",
        "body": [
            "procedure ${1:Name}(${2:Params});"
        ],
        "description": "snippets"
    }, 
    "property-gettter-setter": {
        "prefix": "property-gettter-setter",
        "body": [
            "property ${1:Name}: ${2:string} read Get${1:Name} write Set${1:Name};"
        ],
        "description": "snippets"
    }, 
    "function-begin-end": {
        "prefix": "function-begin-end",
        "body": [
            "function ${1:Name}(${2:Params}):${3:string};",
            "begin",
            "\t$0",
            "end;"
        ],
        "description": "snippets"
    },
    "procedure-begin-end": {
        "prefix": "procedure-begin-end",
        "body": [
            "procedure ${1:Name}(${2:Params});",
            "begin",
            "  $0",
            "end;"
        ],
        "description": "snippets"
    },
    "unit-interface-implementation-end": {
        "prefix": "unit-interface-implementation-end",
        "body": [
            "unit Unit$1;",
            "",
            "{\\$mode objfpc}{\\$H+}",
            "",
            "interface",
            "",
            "uses",
            "  Classes, SysUtils$2;",
            "",
            "implementation",
            "",
            "end."
        ],
        "description": "snippets"
    }
}